{
  "name": "@mherod/get-cookie",
  "version": "2.0.0-rc.31",
  "description": "Node.js module for querying a local user's Chrome cookie",
  "source": "src/index.ts",
  "bin": "dist/cli.js",
  "main": "dist/index.js",
  "module": "dist/module.js",
  "types": "dist/types.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "clean": "git clean -fdX && rm -rf dist && yarn install",
    "lintfix": "prettier --write \"src/**/*.ts\"",
    "prebuild": "npm run lintfix",
    "tsc": "tsc",
    "build": "parcel build --no-cache",
    "prepare": "parcel build --no-cache",
    "test": "jest",
    "prepublish": "npm run build",
    "install:global": "npm run build && npm install --global .",
    "prettier": "prettier --write ."
  },
  "targets": {
    "types": {},
    "main": {
      "source": "src/index.ts",
      "distDir": "dist",
      "isLibrary": true,
      "engines": {
        "node": ">= 16"
      },
      "sourceMap": false,
      "optimize": true,
      "includeNodeModules": {
        "@mapbox/node-pre-gyp": false
      }
    },
    "cli": {
      "source": "src/cli.ts",
      "distDir": "dist",
      "isLibrary": false,
      "engines": {
        "node": ">= 16"
      },
      "sourceMap": false,
      "optimize": true,
      "includeNodeModules": {
        "@mapbox/node-pre-gyp": false
      }
    }
  },
  "@parcel/transformer-js": {
    "inlineFS": false,
    "inlineEnvironment": true
  },
  "engines": {
    "node": ">= 16"
  },
  "keywords": [],
  "author": "Matthew Herod",
  "license": "ISC",
  "dependencies": {
    "consola": "3.2.3",
    "cross-fetch": "^4.0.0",
    "destr": "2.0.2",
    "fast-glob": "^3.3.1",
    "jsonwebtoken": "9.0.0",
    "lodash": "^4.17.21",
    "lodash-es": "^4.17.21",
    "lru-cache": "^7.14.0",
    "minimist": "^1.2.7",
    "sqlite3": "^5.1.6",
    "tough-cookie": "^4.1.2"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@mapbox/node-pre-gyp": "^1.0.11",
    "@parcel/packager-ts": "^2.10.1",
    "@parcel/transformer-typescript-tsc": "^2.10.1",
    "@parcel/transformer-typescript-types": "^2.10.1",
    "@types/jest": "^29.5.6",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/lodash": "^4.14.186",
    "@types/minimist": "^1.2.2",
    "@types/node": "^18.11.4",
    "@types/tough-cookie": "^4.0.2",
    "@types/user-agents": "^1.0.2",
    "jest": "^29.7.0",
    "parcel": "^2.10.1",
    "prettier": "^3.0.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  },
  "parcelDependencies": {
    "@parcel/transformer-typescript-tsc": "^2.10.1",
    "@parcel/transformer-typescript-types": "^2.10.1",
    "@parcel/packager-ts": "^2.10.1"
  }
}

{
  "name": "@mherod/get-cookie",
  "version": "2.1.1",
  "description": "Node.js module for querying a local user's Chrome cookie",
  "packageManager": "pnpm@9.15.2",
  "source": "src/index.ts",
  "bin": "dist/cli/cli.js",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "clean": "git clean -fdX && rm -rf dist && pnpm install",
    "build": "tsc -p tsconfig.json && tsc-alias -p tsconfig.json && chmod +x dist/cli/cli.js",
    "test": "jest",
    "prepublish": "pnpm run build",
    "install:global": "pnpm run build && pnpm install --global .",
    "prettier": "prettier --write .",
    "dev": "tsc -w -p tsconfig.json & tsc-alias -w -p tsconfig.json",
    "lint": "eslint . --ext .ts --format codeframe",
    "lint:fix": "eslint . --ext .ts --format codeframe --fix",
    "read-github": "NODE_OPTIONS=\"-r tsconfig-paths/register\" ts-node -P scripts/tsconfig.json scripts/readGithubCookies.ts"
  },
  "engines": {
    "node": ">= 16"
  },
  "keywords": [],
  "author": "Matthew Herod",
  "license": "ISC",
  "dependencies": {
    "consola": "3.2.3",
    "cross-fetch": "^4.0.0",
    "destr": "2.0.2",
    "fast-glob": "^3.3.1",
    "jsonwebtoken": "9.0.0",
    "lodash": "^4.17.21",
    "lodash-es": "^4.17.21",
    "lru-cache": "^7.14.0",
    "minimist": "^1.2.7",
    "tough-cookie": "^4.1.2",
    "tsconfig-paths": "^4.2.0",
    "zod": "3.24.1"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@types/better-sqlite3": "7.6.12",
    "@types/jest": "^29.5.6",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/lodash": "^4.14.186",
    "@types/minimist": "^1.2.2",
    "@types/node": "^18.19.34",
    "@types/tough-cookie": "^4.0.2",
    "@types/user-agents": "^1.0.2",
    "@typescript-eslint/eslint-plugin": "8.19.0",
    "@typescript-eslint/parser": "8.19.0",
    "better-sqlite3": "11.7.0",
    "eslint": "^8.56.0",
    "eslint-formatter-codeframe": "7.32.1",
    "eslint-import-resolver-typescript": "3.7.0",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-jsdoc": "50.6.1",
    "jest": "^29.7.0",
    "prettier": "^3.0.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "tsc-alias": "1.8.10",
    "tsup": "8.3.5",
    "typescript": "^5.1.6"
  }
}
